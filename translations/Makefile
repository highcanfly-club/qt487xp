ts:
	@echo "The 'ts' target has been removed in favor of more fine-grained targets." && echo "Use 'ts-<target>-<lang>' or 'ts-<lang>' instead. To add a language," && echo "use 'untranslated' for <lang>, rename the files and re-run 'qmake'."

commit-ts:
	cd C:\qt487xp\translations\.. && for /f usebackq %%f in (`git diff-files --name-only translations/*_??.ts`) do C:\qt487xp\bin\lconvert.exe -locations none -i %%f -o %%f 
	 cd C:\qt487xp\translations\.. && git add translations/*_??.ts && git commit

check-ts: ts-all
	(cd C:/qt487xp/translations && perl check-ts.pl)

sis:

check: first
FORCE:

$(MAKEFILE).Debug: Makefile
$(MAKEFILE).Release: Makefile
